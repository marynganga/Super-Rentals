<li>
	{{#link-to 'rental' rental.id}}
		{{rental.owner}}'s {{rental.type}} in {{rental.city}}
	{{/link-to}}
	{{#if isImageShowing}}
		<p><img src={{rental.image}} alt={{rental.type}} {{action 'imageHide'}}>
		</p>
	{{else}}
		<button class="btn btn-info" {{action 'imageShow'}}>Show Image</button>
	{{/if}}
</li>